<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UCcontrol</servlet-name>
        <servlet-class>miServlet.UCcontrol</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uccontrol</servlet-name>
        <servlet-class>miServlet.uccontrol</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>svRegistro</servlet-name>
        <servlet-class>miServlet.svRegistro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>svReserva</servlet-name>
        <servlet-class>miServlet.svReserva</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>svAdicionCliente</servlet-name>
        <servlet-class>miServlet.svAdicionCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uacontrol</servlet-name>
        <servlet-class>miServlet.uacontrol</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UCcontrol</servlet-name>
        <url-pattern>/UCcontrol</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uccontrol</servlet-name>
        <url-pattern>/uccontrol</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>svRegistro</servlet-name>
        <url-pattern>/svRegistro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>svReserva</servlet-name>
        <url-pattern>/svReserva</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>svAdicionCliente</servlet-name>
        <url-pattern>/svAdicionCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uacontrol</servlet-name>
        <url-pattern>/uacontrol</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
